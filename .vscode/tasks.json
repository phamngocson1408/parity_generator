{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Activate venv",
            "type": "shell",
            "command": "source",
            "args": [
                "${workspaceFolder}/.venv39/bin/activate"
            ],
            "runOptions": {
                "runOn": "folderOpen"
            },
            "presentation": {
                "reveal": "never"
            }
        },
        {
            "label": "Generate Parity: BOS_AXICRYPT (SAFETY.PARITY)",
            "type": "shell",
            "command": "${workspaceFolder}/.venv312/bin/python3",
            "args": [
                "${workspaceFolder}/parity_generator.py",
                "-info",
                "${workspaceFolder}/[INFO]_BOS_AXICRYPT.safety.xlsx",
                "-type",
                "SAFETY.PARITY"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "shared",
                "focus": false
            },
            "problemMatcher": []
        },
        {
            "label": "Generate Parity: SIMPLE_TOP (SAFETY.PARITY)",
